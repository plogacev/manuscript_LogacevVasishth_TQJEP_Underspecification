journal = {Behavioural and Brain Sciences},
keywords = {cognition,humans,individuality,linguistics,memory,memory disorders,memory disorders psychology,models,neurological,psychological,review,sentencecomprehension,short term,short term classification,verbal learning,verbal learning classification,workingmemory},
