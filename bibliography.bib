keywords = {Algorithms,Animals,Association,Concept Formation,Discrimination (Psychology),Humans,Likelihood Functions,Markov Chains,Models,Monte Carlo Method,Psychological,Research Design,Research Design: statistics \& numerical data,Retention (Psychology),Uncertainty},
number = {3},
pages = {499--518},
pmid = {19618983},
title = {{Optimal experimental design for model discrimination.}},
volume = {116},
year = {2009}
}
