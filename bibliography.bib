@article{AgrestiCoull1998,
abstract = {For interval estimation of a proportion, coverage probabilities tend
to be too large for "exact" confidence intervals based on inverting
the binomial test and too small for the interval based on inverting
the Wald large-sample normal test (i.e., sample proportion ± z-score
