number = {2},
pages = {176--183},
pmid = {16977449},
title = {{LATER predicts saccade latency distributions in reading}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16977449 http://www.springerlink.com/index/34T49687MQ5R7836.pdf},
