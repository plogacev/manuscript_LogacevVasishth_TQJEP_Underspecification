pages = {235--248},
pmid = {12820864},
title = {{Event-related brain potentials reflect discourse-referential ambiguity in spoken language comprehension}},
volume = {40},
year = {2003}
}
@article{Crick1984,
abstract = {It is suggested that in the brain the internal attentional searchlight,
