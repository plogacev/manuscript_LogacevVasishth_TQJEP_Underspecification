issn = {0749596X},
journal = {Journal of Memory and Language},
keywords = {Kurs RT,accuracy tradeoff,sentence processing,speed,verb-phrase ellipsis},
number = {3},
pages = {879--906},
title = {{A content-addressable pointer mechanism underlies comprehension of verb-phrase ellipsis}},
volume = {58},
year = {2008}
}
