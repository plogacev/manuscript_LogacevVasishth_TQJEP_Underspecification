journal = {Current Directions in Psychological Science},
keywords = {humanparsing,sentenceprocessing,shallowparsing,theory},
number = {1},
pages = {11--15},
publisher = {Blackwell Publishers Inc.},
title = {{Good-Enough Representations in Language Comprehension}},
volume = {11},
year = {2002}
}
