journal = {Cogntion},
number = {2},
pages = {171--181},
pmid = {19101668},
title = {{Tasty non-words and neighbours: the cognitive roots of lexical-gustatory synaesthesia}},
url = {http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?db=pubmed\&cmd=Retrieve\&dopt=AbstractPlus\&list\_uids=19101668},
