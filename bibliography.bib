@article{Bailey2008a,
abstract = {We introduce and empirically evaluate the strategy affordance hypothesis, which holds that individual differences in strategy use will mediate the relationship between performances on a working memory (WM) span task and another cognitive task only when the same strategies are afforded by both tasks. One hundred forty-eight participants completed basic memory tasks and verbal span tasks that afford the same strategies, such as imagery and sentence generation, and completed reading comprehension tasks that afford different ones, such as self-questioning and summarization. Effective strategy use on WM span tasks accounted for variance in the span-memory relationship, but not for the span-comprehension relationship, supporting the strategy affordance hypothesis. Strategy use mediated the span-cognition relationship only when both tasks afforded the same strategies.},
annote = {- strategy affordance hypothesis
- strategy use only mediates relationship between wm and other tasks when the same strategies are afforded by both tasks

        
- empirical evidence: span performance higher when individuals report effective strategies than when they report less effective strategies
- turley-ames \{\&\} whitfield (2003): strategy-use in OSPAN does not predict performance in NDRT
> inconsistent with general strategy mediation hypothesis (mcnamara \{\&\} scott 2001): diff. in strategy-use entirely/subtantially mediate rel. between span and criterion
- alt: strategy affordance hypothesis (OSPAN single word, nelson-denny completely different strategies necessary)

        
- prediction sah: strategy-use will not mediate rel. span tasks and ndrt
- ospan, rspan
- set-by-set strategy reports
- criterion: free recall, paired-associate with same strategy; ndrt and sat different strategies
- strategy-use should mediate corr. between span and memory tasks, but not between span and comprehension
- two sessions, one task of all kinds each

        
results
- two categories for strat: effective (interactive imagery, sentence generation, grouping) and less effective (passive reading, rote repetition)
- effective: 28\% ospan, 26\% rspan
- different strategies for ndrt and sat (skimming, reading questions first, etc.)
- ospan: score higher when strategy effective, same pattern in rspan and memory tasks
- strategy-use in span tasks sig. related to performance on mem. tasks > may be explanation
- not sig. related to performance in compr. tasks
> inconsistent with general mediation hypothesis
- strategy use sig. contributed to to correlation with memory performance over and above what was predicted by span score

        
disc.
- clear evidence for stra. aff. hyp.: strategy not responsible for all span-cog. relationships
- to what extent does the capability to generate and implement effective strategies cause the stat. rel. betw. strategy use and span performance?
> strategy-as-cause hypothesis: individuals who are more strategic achieve higher scoores
> strategy-as-effect hypthesis: higher span individuals have resources to implement additional strategies which additionally contributes to span scores},
author = {Bailey, Heather and Dunlosky, John and Kane, Michael J},
doi = {10.3758/MC.36.8.1383},
file = {:home/pavel/Documents/Mendeley Desktop/Bailey, Dunlosky, Kane/Bailey, Dunlosky, Kane\_2008\_Memory \& Cognition.pdf:pdf},
journal = {Memory \& Cognition},
keywords = {Cognition,Female,Humans,Male,Memory: Short-Term,Prospective Studies,Psychological Theory,Reaction Time,Young Adult,workingmemory},
mendeley-tags = {workingmemory},
number = {8},
pages = {1383--1390},
